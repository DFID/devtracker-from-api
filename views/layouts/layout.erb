    <script> 
    $(document).ready(function() {
         $(".more-info-link").click(function(e){
            e.stopPropagation();
            $("#moreinfo"+$(this).attr('target')).toggle();
           });
         
         $(".more-info").on('click', function(e){
            e.stopPropagation();
          });

         $(document).on('click', function(e){
            $(".more-info").hide(); 
          });           
      });
  </script> 
