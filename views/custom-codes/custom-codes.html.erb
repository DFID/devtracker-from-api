<div class="govuk-breadcrumbs govuk-breadcrumbs--collapse-on-mobile">
    <ol class="govuk-breadcrumbs__list">
        <li class="govuk-breadcrumbs__list-item"><a class="govuk-breadcrumbs__link" href="/">Home</a></li>
        <li class="govuk-breadcrumbs__list-item" aria-current="page">Custom Codes</li>
    </ol>
</div>

<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-l">Custom Codes & Vocabularies</h1>
        <h2 class="govuk-heading-m" id="<%= policyPriorities['metadata']['name'].downcase.gsub!(" ","-")%>"><%= policyPriorities['metadata']['name']%></h2>
        <p class="govuk-body"><a class="anchor" href="#<%= policyPriorities['metadata']['name'].downcase.gsub!(" ","-")%>" title="Permalink to this headline" class="govuk-link">View codes</a></p>
        <div class="govuk-body"><%= policyPriorities['metadata']['description']%></div>
        <%if policyPriorities['data'].length > 0%>
            <table class="custom-codes display govuk-!-margin-top-6" style="width:100%; display: none">
                <thead>
                    <tr>
                        <th><span>Code</span></th>
                        <th><span>Name</span></th>
                        <th><span>Description</span></th>
                        <th><span>Status</span></th>
                    </tr>
                </thead>
                <tbody>
                    <%policyPriorities['data'].each do |data|%>
                        <tr>
                            <td><%= data['Code']%></td>
                            <td><%= data['Name']%></td>
                            <td><%= data['Description']%></td>
                            <td><%= data['Status']%></td>
                        </tr>
                    <%end%>
                </tbody>
            </table>
        <%end%>        
    </div>
</div>

<script src="/javascripts/jquery-3.3.1.js" type="text/javascript"></script>
<script src="/javascripts/jquery.dataTables.min.js" type="text/javascript"></script>
<script src="/javascripts/dataTables.buttons.min.js" type="text/javascript"></script>
<script src="/javascripts/buttons.flash.min.js" type="text/javascript"></script>
<script src="/javascripts/buttons.html5.min.js" type="text/javascript"></script>
<script src="/javascripts/buttons.print.min.js" type="text/javascript"></script>
<link rel="stylesheet" type="text/css" href="/stylesheets/jquery.dataTables.min.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/buttons.dataTables.min.css">
<link type="text/css" rel="stylesheet" href="/stylesheets/jquery-ui-1.10.2.custom.min.css" >
<script type="text/javascript">
  var jq3 = jQuery.noConflict(true);
  jq3(document).ready(function() {
    jq3('.custom-codes').DataTable({
      "info":false,
      "paging":false,
      "searching":false,
      "order":[1,"asc"]
    });
    jq3('.custom-codes').show();
  })
</script>